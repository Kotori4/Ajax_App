<h1>TOP PAGE</h1>
<%= link_to "新規投稿", "/posts/new"%>

<% @posts.each do |post| %>
  <div class="post">
    <div class="post-date">
      POSTED DATE&TIME<%= post.created_at %>
    </div>
    <div class="post-content">
      <%= post.content %>
    </div>
</div>
<% end %>




